{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-typescript/base"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-console": "off",
      "@typescript-eslint/semi": ["error", "always"]
    }
  }
  